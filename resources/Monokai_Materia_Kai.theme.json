      "Toolbar.borderColor": "bg_dk",
      "Header.activeBackground": "bg",
      "Header.inactiveBackground": "bg",
      "Advertiser": {
        "foreground": "bg_vlt",
        "background": "bg",
        "borderColor": "bg_vlt",
        "borderInsets": "4,8,3,0"
