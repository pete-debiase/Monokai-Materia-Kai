      "lightSelectionInactiveBackground": "bg",
      "lightSelectionInactiveForeground":"fg",

      "disabledBackground": "bg",
      "inactiveBackground": "bg",

      "disabledForeground": "cmt",
      "disabledText": "cmt",
      "inactiveForeground": "cmt",

      "acceleratorForeground": "cmt",
      "acceleratorSelectionForeground": "cmt",

      "errorForeground": "red",

      "borderColor": "bg_vlt",
      "disabledBorderColor": "bg_vlt",

      "focusColor": "bg_vlt",
      "focusedBorderColor": "bg_vlt",

      "separatorForeground": "bg_vlt",
      "separatorColor": "bg_vlt",
      "lineSeparatorColor": "bg_vlt",

      "modifiedItemForeground": "purple"
    },

    "ActionButton": {
      "hoverBackground": "slkn",
